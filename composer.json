{
  "name": "aoe/aoe-dbsequenzer",
  "description": "With this extension you can ensure different unique keys for the configured tables.",
  "type": "typo3-cms-extension",
  "authors": [
      {
          "name": "AOE GmbH",
          "email": "dev@aoe.com"
      }
  ],
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": ">=6.2.0",
    "typo3/cms-extbase": ">=6.2.0",
    "typo3/cms-sv": ">=6.2.0",
    "typo3/cms-backend": ">=6.2.0",
    "typo3/cms-reports": ">=6.2.0",
    "typo3/cms-frontend": ">=6.2.0"
  },
  "require-dev": {
    "typo3/cms": "6.2.*",
    "typo3-ter/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "*",
    "sebastian/phpcpd": "*",
    "phpmd/phpmd": "*",
    "phpunit/phpunit": "4.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TYPO3-extensions/phpunit.git"
    },
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "autoload": {
    "classmap": ["Classes/", "Tests"]
  }
}